<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/news_feed_list_view"
        android:layout_width="417dp"
        android:layout_height="229dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/news_feed_username"
        android:layout_width="40dp"
        android:layout_height="37dp"
        android:layout_marginStart="69dp"
        android:layout_marginEnd="308dp"
        android:layout_marginBottom="192dp"
        android:paddingTop="5dp"
        android:text="Joe"
        app:layout_constraintBottom_toBottomOf="@+id/news_feed_list_view"
        app:layout_constraintEnd_toEndOf="@+id/news_feed_list_view"
        app:layout_constraintStart_toStartOf="@+id/news_feed_list_view"
        app:layout_constraintTop_toTopOf="@+id/news_feed_list_view" />

    <TextView
        android:id="@+id/news_feed_date"
        android:layout_width="wrap_content"
        android:layout_height="37dp"
        android:layout_marginStart="134dp"
        android:layout_marginEnd="209dp"
        android:layout_marginBottom="192dp"
        android:paddingTop="5dp"
        android:text="TextView"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/news_feed_list_view"
        app:layout_constraintEnd_toEndOf="@+id/news_feed_list_view"
        app:layout_constraintStart_toStartOf="@+id/news_feed_list_view"
        app:layout_constraintTop_toTopOf="@+id/news_feed_list_view" />

    <ImageView
        android:id="@+id/news_feed_user_image"
        android:layout_width="59dp"
        android:layout_height="37dp"
        android:layout_marginEnd="358dp"
        android:layout_marginBottom="192dp"
        app:layout_constraintBottom_toBottomOf="@+id/news_feed_list_view"
        app:layout_constraintEnd_toEndOf="@+id/news_feed_list_view"
        app:layout_constraintStart_toStartOf="@+id/news_feed_list_view"
        app:layout_constraintTop_toTopOf="@+id/news_feed_list_view"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/news_feed_text"
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="37dp"
        android:layout_marginBottom="173dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/news_feed_list_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/news_feed_list_view" />

    <ImageView
        android:id="@+id/news_feed_image"
        android:layout_width="360dp"
        android:layout_height="127dp"
        android:layout_marginBottom="46dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/news_feed_list_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/news_feed_text"
        tools:srcCompat="@tools:sample/backgrounds/scenic" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="45dp"
        android:layout_height="36dp"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="346dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/news_feed_list_view"
        app:layout_constraintEnd_toEndOf="@+id/news_feed_list_view"
        app:layout_constraintStart_toStartOf="@+id/news_feed_list_view"
        app:layout_constraintTop_toBottomOf="@+id/news_feed_image"
        app:srcCompat="@android:drawable/btn_star_big_off" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="48dp"
        android:layout_height="36dp"
        android:layout_marginStart="118dp"
        android:layout_marginEnd="251dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/news_feed_list_view"
        app:layout_constraintEnd_toEndOf="@+id/news_feed_list_view"
        app:layout_constraintStart_toStartOf="@+id/news_feed_list_view"
        app:layout_constraintTop_toBottomOf="@+id/news_feed_image"
        app:srcCompat="@android:drawable/sym_action_chat" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="59dp"
        android:layout_height="36dp"
        android:layout_marginStart="208dp"
        android:layout_marginEnd="153dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/news_feed_list_view"
        app:layout_constraintEnd_toEndOf="@+id/news_feed_list_view"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/news_feed_list_view"
        app:layout_constraintTop_toBottomOf="@+id/news_feed_image"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@android:drawable/ic_menu_share" />

    <TextView
        android:id="@+id/likes_number"
        android:layout_width="24dp"
        android:layout_height="17dp"
        android:layout_marginTop="138dp"
        android:layout_marginBottom="18dp"
        android:text="0"
        android:textAlignment="viewStart"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/news_feed_list_view"
        app:layout_constraintStart_toEndOf="@+id/imageButton2"
        app:layout_constraintTop_toBottomOf="@+id/news_feed_text" />

    <TextView
        android:id="@+id/comments_number"
        android:layout_width="24dp"
        android:layout_height="17dp"
        android:layout_marginTop="138dp"
        android:layout_marginBottom="18dp"
        android:text="0"
        android:textAlignment="viewStart"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/news_feed_list_view"
        app:layout_constraintStart_toEndOf="@+id/imageButton3"
        app:layout_constraintTop_toBottomOf="@+id/news_feed_text" />

    <TextView
        android:id="@+id/reshares_number"
        android:layout_width="24dp"
        android:layout_height="17dp"
        android:layout_marginTop="138dp"
        android:layout_marginEnd="129dp"
        android:layout_marginBottom="18dp"
        android:text="0"
        android:textAlignment="viewStart"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/news_feed_list_view"
        app:layout_constraintEnd_toEndOf="@+id/news_feed_list_view"
        app:layout_constraintStart_toEndOf="@+id/imageButton4"
        app:layout_constraintTop_toBottomOf="@+id/news_feed_text" />

</androidx.constraintlayout.widget.ConstraintLayout>